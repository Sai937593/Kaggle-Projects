    "def hist_plot_cat_cols(df:pd.DataFrame(), cat_cols=cat_cols):\n",
    "    df = df.copy()\n",
    "    fig, axes_cat = plt.subplots(1, len(cat_cols), figsize=(20, 10))\n",
    "    for i, col in enumerate(cat_cols):\n",
    "        axes_cat[i].hist(df[col])\n",
    "        axes_cat[i].set_title(col)"
